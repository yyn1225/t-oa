<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jtech.toa.dao.SalesMapper">
    <select id="selectSalesByArea" resultType="com.jtech.toa.entity.product.Sales">
        SELECT * FROM absen_product_sales WHERE product = #{product} AND area = #{area}
    </select>
</mapper>
